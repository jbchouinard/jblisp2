(import-from "stl/unittest"
    test
    test-suite
    assert-raise
    assert-equal
    assert-eq
)

(test-suite "base"
    (test "asserting false things raises"
        (assert-raise (assert false)))

    (test "not true is false"
        (assert-equal true (not false)))

    (test "nil is nil"
        (assert-eq () (list)))

    (test "true is true"
        (assert-eq (not false) (not false)))
)